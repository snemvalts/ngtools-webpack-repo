{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true
  }
}